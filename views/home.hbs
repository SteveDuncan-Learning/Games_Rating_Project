{{!-- <div class="container wrapper"> --}}
<div class="container dashboard">
    <div class='dash_nav'>
        {{>dashboard_nav}}
    </div>
    <div class="dash_body container wrapper">
        {{#each articles}}
        <a href="/games/{{this._id}}">
            <div class="item">
                <div class="top">
                    <div class="title">{{this.title}}</div>
                </div>
                <div class="content">
                    {{this.overview}}
                </div>
                <div class="bottom">
                    <div class="rating">
                        <span>Average rating: </span>{{this.aveRating}}
                    </div>
                </div>
            </div>
        </a>
        {{/each}}
    </div>
    </div>
</div>